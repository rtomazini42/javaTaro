<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.inicioController">
   <children>
      <ImageView disable="true" fitHeight="600.0" fitWidth="800.0" layoutX="-13.0" layoutY="-3.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/fundo_madeira.jpg" />
         </image>
      </ImageView>
      <Button fx:id="botaoIniciar" blendMode="COLOR_DODGE" layoutX="312.0" layoutY="300.0" mnemonicParsing="false" onAction="#iniciar" text="Iniciar" textAlignment="CENTER">
         <font>
            <Font name="Franklin Gothic Medium" size="44.0" />
         </font>
      </Button>
      <Button fx:id="sair" blendMode="COLOR_DODGE" layoutX="333.0" layoutY="421.0" mnemonicParsing="false" onAction="#saida" text="Sair" textAlignment="CENTER">
         <font>
            <Font name="Franklin Gothic Medium" size="44.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
